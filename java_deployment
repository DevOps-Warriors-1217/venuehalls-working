apiVersion: apps/v1
kind: Deployment
metadata:
  name: service-deployment
  namespace: vedika
spec:
  selector:
    matchLabels:
      app: service
  replicas: 4 # tells deployment to run 2 pods matching the template
  template:
    metadata:
      labels:
        app: service
    spec:
      containers:
      - name: service
        image: shivastunts/vedika-service-testing-fullcode:v1
        ports:
        - containerPort: 8057

